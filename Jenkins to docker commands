    2  sudo systemctl start docker
    3  sudo systemctl enable docker
    4  sudo vi dockerfile
    5  docker build -t online .
    6  sudo chmod 666 /var/run/docker.sock
    7  docker build -t online .
    8  docker images
    9  docker run -dt --name online -p 8080:8080 online
   10  sudo -i
